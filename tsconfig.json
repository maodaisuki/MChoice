{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "mchoice": ["src"],
      "mchoice/*": ["src/*", "*"]
    },
    "target": "ESNext"
  },
  "include": [".dumirc.ts", "src/**/*"]
}
